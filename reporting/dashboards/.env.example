# React Environment Variables (.env.example)
# Copy this file to .env and fill in your actual API keys
# Never commit .env file to version control

# API Keys for Financial Markets
REACT_APP_ALPHAVANTAGE_API_KEY=your_alphavantage_key_here
REACT_APP_MARKETSTACK_API_KEY=your_marketstack_key_here  
REACT_APP_POLYGON_API_KEY=your_polygon_key_here

# News and Information APIs
REACT_APP_NEWSAPI_KEY=your_newsapi_key_here

# Cybersecurity APIs
REACT_APP_ABUSEIPDB_API_KEY=your_abuseipdb_key_here
REACT_APP_VIRUSTOTAL_API_KEY=your_virustotal_key_here

# NASA and Space APIs
REACT_APP_NASA_API_KEY=your_nasa_key_here

# Weather API
REACT_APP_OPENWEATHERMAP_API_KEY=your_weather_key_here

# Backend API Configuration
REACT_APP_BACKEND_URL=http://localhost:5001
REACT_APP_WEBSOCKET_URL=ws://localhost:5001

# Enterprise System Configuration
REACT_APP_ENTERPRISE_API_URL=http://localhost:8000
REACT_APP_AGENT_MONITORING_URL=ws://localhost:8000/agents
REACT_APP_WORKFLOW_MONITORING_URL=ws://localhost:8000/workflows
